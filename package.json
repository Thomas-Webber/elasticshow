	"devDependencies": {
		"esbuild": "^0.8.52"
	},
	"files": [
		"lib/"
	],
	"scripts": {
		"watch": "NODE_ENV=development webpack-dev-server --open --mode development --content-base dist/app --history-api-fallback --hot --colors --port 1342",
		"watch:site": "NODE_ENV=development webpack-dev-server --mode development --content-base dist/site --history-api-fallback --hot --progress --colors --port 1359 --config webpack.config.site.js",
		"build": "yarn build:app && yarn build:site",
		"build:app": "NODE_ENV=production webpack",
		"build:site": "NODE_ENV=product webpack --config webpack.config.site.js",
		"build:chrome-extension": "yarn build:app && cd dist/app && rm favicon/manifest.json _redirects && zip -r ../dejavu.zip .",
		"format": "prettier --write --use-tabs 'app/src/!(batteries*)**/**/*.js'",
		"precommit": "lint-staged"
	},
	"browser": {
		"path": "path-browserify"
	}
}